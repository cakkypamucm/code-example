    "private": true,
    "main": "src/index.js",
    "scripts": {
        "start": "webpack serve --node-env development",
        "git-update-index": "git update-index --again",
        "gh-pages-create": "node src/helpers/build/gh-pages/create.cjs",
        "gh-pages-build": "node src/helpers/build/gh-pages/build.cjs",
        "husky-install": "node src/helpers/build/husky-init.cjs",
        "build-browserslist-regexp": "(echo export default && browserslist-useragent-regexp --ignoreMinor --allowHigherVersions) > src/helpers/supported-browsers-regexp.js",
        "eslint": "eslint --cache --fix \"src/**/*.{cjs,js,vue,md}\" \"*.{cjs,js,md}\"",
        "stylelint": "stylelint --cache --fix \"src/**/*.{css,scss,vue}\"",
        "prettier": "prettier --cache --cache-location .prettiercache --find-config-path --write \"{src,public}/**/*.{css,scss,cjs,js,json,vue,md,html}\" \"*.{cjs,js,json,md}\" \"!(package)*.json\"",
        "husky-precommit": "run-s lint git-update-index",
        "lint": "run-s eslint stylelint prettier",
        "postinstall": "run-p gh-pages-create husky-install"
    },
    "author": "Denis S. <cakkypamucm@yandex.ru>",
